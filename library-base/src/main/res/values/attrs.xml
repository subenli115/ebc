<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NormalTitleBar">
        <attr name="ntb_bg" format="reference"/>
        <attr name="ntb_right_img1" format="reference" />
        <attr name="ntb_right_img2" format="reference" />
        <attr name="ntb_right_img3" format="reference" />
        <attr name="ntb_right_text" format="string" />
        <attr name="ntb_title_text" format="string" />
        <attr name="ntb_right_text_color" format="color" />
        <attr name="ntb_right_text_size" format="float" />
        <attr name="ntb_right_img_size" format="dimension" />
        <attr name="ntb_img_padding" format="dimension" />
        <attr name="ntb_tv_left" format="enum" >
            <enum name="show" value="0" />
            <enum name="showAndFinish" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <!-- 描边宽度-->
        <attr name="border_width1" format="dimension"/>
        <!-- 描边颜色-->
        <attr name="border_color1" format="color"/>
        <!--圆角大小-->
        <attr name="corner_radius" format="dimension"/>
        <!--左上圆角大小-->
        <attr name="leftTop_corner_radius" format="dimension"/>
        <!--右上圆角大小-->
        <attr name="rightTop_corner_radius" format="dimension"/>
        <!--左下圆角大小-->
        <attr name="leftBottom_corner_radius" format="dimension"/>
        <!--右下圆角大小-->
        <attr name="rightBottom_corner_radius" format="dimension"/>
        <!--图片类型：圆角或者圆形-->
        <attr name="roun_type" format="enum">
            <enum name="oval" value="2"/>
            <enum name="round" value="1"/>
            <enum name="circle" value="0"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SwitchButton">
        <attr name="sb_shadow_radius" format="reference|dimension"/>
        <attr name="sb_shadow_offset" format="reference|dimension"/>
        <attr name="sb_shadow_color" format="reference|color"/>
        <attr name="sb_uncheck_color" format="reference|color"/>
        <attr name="sb_checked_color" format="reference|color"/>

        <attr name="sb_border_width" format="dimension"/>
        <attr name="sb_checkline_color" format="reference|color"/>
        <attr name="sb_checkline_width" format="reference|dimension"/>
        <attr name="sb_uncheckcircle_color" format="reference|color"/>
        <attr name="sb_uncheckcircle_width" format="reference|dimension"/>
        <attr name="sb_uncheckcircle_radius" format="reference|dimension"/>

        <attr name="sb_checked" format="boolean"/>
        <attr name="sb_shadow_effect" format="reference|boolean"/>
        <attr name="sb_effect_duration" format="reference|integer"/>
        <attr name="sb_button_color" format="reference|color"/>
        <attr name="sb_show_indicator" format="reference|boolean"/>
        <attr name="sb_background" format="reference|color"/>
        <attr name="sb_enable_effect" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="rightIconDel" format="reference"/>
        <attr name="rightIconShowPwd" format="reference"/>
        <attr name="rightIconHidePwd" format="reference"/>
        <attr name="type" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="checked" format="boolean"/>
        <attr name="color_unchecked_stroke" format="color"/>

    </declare-styleable>
    <declare-styleable name="ItemGroupView">
        <attr name="itemView_leftText" format="string"/>
        <attr name="itemView_leftHintText" format="string"/>
        <attr name="itemView_rightText" format="string"/>
        <attr name="itemView_leftText_color" format="color"/>
        <attr name="itemView_rightText_color" format="color"/>
        <attr name="itemView_rightTextSize" format="dimension"/>
        <attr name="itemView_leftTextSize" format="dimension"/>
        <attr name="showBottomLine" format="boolean"/>
        <attr name="showRightArrow" format="boolean"/>
        <attr name="showRightImage" format="boolean"/>
        <attr name="showSlideSwitch" format="boolean"/>
        <attr name="rightIcon" format="reference"/>
        <attr name="leftIcon" format="reference"/>
    </declare-styleable>
    <declare-styleable name="TwinklingRefreshLayout">
        <attr name="tr_wave_height" format="dimension" />
        <attr name="tr_head_height" format="dimension" />
        <attr name="tr_bottom_height" format="dimension" />
        <attr name="tr_overscroll_height" format="dimension" />
        <attr name="tr_enable_loadmore" format="boolean" />
        <attr name="tr_pureScrollMode_on" format="boolean" />
        <attr name="tr_show_overlay_refreshview" format="boolean" />
        <attr name="tr_headerView" format="dimension" />
        <attr name="tr_bottomView" format="dimension" />
        <attr name="onRefreshCommand" format="reference" />
        <attr name="onLoadMoreCommand" format="reference" />
    </declare-styleable>
    <declare-styleable name="BaseItemView">
        <attr name="biv_title" format="string"/>
        <attr name="biv_title_size" format="float"/>
        <attr name="biv_right_text" format="string"/>
        <attr name="biv_title_width" format="dimension"/>
        <attr name="biv_right_color" format="color"/>
        <attr name="biv_title_color" format="color"/>
        <attr name="biv_click_id" format="integer"/>
        <attr name="biv_right_click_id" format="integer"/>
        <attr name="biv_content_type" format="enum">
            <enum name="text" value="0"/>
            <enum name="edit" value="1"/>
            <enum name="image" value="2"/>
            <enum name="sex" value="3"/>
            <enum name="right_box" value="4"/>
        </attr>
        <attr name="biv_content_image_size" format="dimension"/>
        <attr name="biv_content_image_res" format="reference|color"/>
        <attr name="biv_content_hint" format="string"/>
        <attr name="biv_content_max_line" format="integer"/>
        <attr name="biv_forword_show" format="boolean"/>
        <attr name="biv_divider_show" format="boolean"/>
        <attr name="biv_content_must_need" format="boolean"/>
        <attr name="biv_content_color" format="color"/>
        <attr name="biv_content_size" format="float"/>
        <attr name="biv_content_ime" format="enum">
            <enum name="next" value="5" />
            <enum name="send" value="4" />
            <enum name="done" value="6" />
        </attr>
        <attr name="biv_content_input_type" format="enum">
            <enum name="text" value="0" />
            <enum name="phone" value="1" />
            <enum name="number" value="2" />
            <enum name="email" value="3" />
        </attr>
        <attr name="biv_right_text_size" format="float"/>
        <attr name="biv_content_margin_left" format="dimension"/>
        <attr name="biv_background" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="MyCheckBox">
        <attr name="mcb_duration" format="integer"/>
        <attr name="mcb_stroke_width" format="dimension"/>
        <attr name="mcb_open_animation" format="boolean"/>
        <attr name="mcb_color_tick" format="color"/>
        <attr name="mcb_scb_checked" format="boolean"/>
        <attr name="mcb_color_checked" format="color"/>
        <attr name="mcb_color_unchecked" format="color"/>
        <attr name="mcb_color_unchecked_stroke" format="color"/>
        <attr name="mcb_extra_text" format="string" />
        <attr name="mcb_extra_text_size" format="dimension" />
        <attr name="mcb_extra_text_color" format="color" />
    </declare-styleable>
</resources>